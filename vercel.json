{
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 10,
        "buildCommand": "python manage.py collectstatic --noinput"
      }
    }
  ],
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "DJANGO_SETTINGS_MODULE": "myproject.settings"
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}